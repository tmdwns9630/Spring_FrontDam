			$(document).ready(function(){
				var keywordSlider = $('.keywordSlider').slick({
					infinite: false,
					dots: true,
					arrows:false,
					slidesToShow: 1,
					slidesToScroll: 1
				});
				
				$(".section2 ul li a").click(function(){
					t = $(this).parent('li');
					if (t.hasClass('active')) {
						t.removeClass('active');
					}else {
						$(".section2 ul li").removeClass('active');
						t.addClass('active');
						return false;
					}
				});

				 $('.section2 ul li a').click(function(e) {
				   e.preventDefault();
				   var slideno = $(this).data('slide');
				   keywordSlider.slick('slickGoTo', slideno - 1);
				 });
			});
